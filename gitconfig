[user]
	name = Michael Hall
	email = michael@mbh.sh
[credential]
	helper = store
[alias]
    co = checkout
    br = branch
    last = log -1 HEAD
    abbrev = log --abbrev-commit
[pull]
	rebase = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[init]
	defaultBranch = main
[push]
	default = simple
